VITE_BUZZY_WEB_APP_CLIENT_ID=buzzy-webapp
VITE_BUZZY_WEB_APP_CLIENT_SECRET=buzzy-webapp-secret

VITE_API_VERSION=/v1

VITE_SSO_SERVER_URL=http://login.buzzy.io
VITE_API_SERVER_URL=http://api.buzzy.io
VITE_WS_SERVER_URL=ws://ws.buzzy.io/ws

VITE_SIGNUP_ENDPOINT=/signup
VITE_TOKENS_ENDPOINT=/oauth2/token
