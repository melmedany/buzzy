VITE_MESSAGE_STATE_DELIVERED_DESTINATION=/conversations/{conversationId}/messages/{messageId}/delivered/
VITE_MESSAGE_STATE_READ_DESTINATION=/conversations/{conversationId}/messages/{messageId}/read/
